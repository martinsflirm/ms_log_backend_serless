{
    "version": 2,
    "builds": [
      {
        "src": "api/main.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/api/main.py"
      }
    ]
  }